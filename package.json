    "@types/mocha": "2.2.44",
    "@types/node": "8.0.58",
    "@types/pino": "4.7.0",
    "@types/supertest": "2.0.4",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "supertest": "3.0.0",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.2"
  }
